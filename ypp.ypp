%{
#include "hpp.hpp"
%}
%defines %union { Sym*o; }
%token <o> SYM
%token <o> LP RP COMMA
%type <o> ex vec
%%
REPL : | REPL ex	{ cout << $2->dump() << endl; } ;
ex : SYM
	| SYM LP vec RP	{ $$=$1; $$->push($3); }
;
vec :				{ $$=new Vector(); }
	| vec ex		{ $$=$1; $$->push($2); }
	| vec COMMA ex	{ $$=$1; $$->push($3); }
;
%%