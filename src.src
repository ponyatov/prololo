skelex = [
	files = [ src.src log.log ypp.ypp lpp.lpp hpp.hpp cpp.cpp ]
	Makefile .gitignore
]

.gitignore = [ *~ *swp *exe *log ypp.tab.?pp lex.yy.c ]

Makefile = [
log.log : src.src ./exe.exe
	./exe.exe < $< > $@ && tail $(TAIL) $@
C = cpp.cpp ypp.tab.cpp lex.yy.c
H = hpp.hpp ypp.tab.hpp
CXXFLAGS += -std=gnu++11
./exe.exe : $(C) $(H)
	$(CXX) $(CXXFLAGS) -o $@ $(C)
ypp.tab.cpp : ypp.ypp
	bison $<
lex.yy.c : lpp.lpp
	flex $<
]
